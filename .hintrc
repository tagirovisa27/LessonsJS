{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": "off",
    "highest-available-document-mode": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "button-type": "off",
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-moz-appearance",
          "@-ms-viewport",
          "@-ms-viewport"
        ]
      }
    ]
  }
}